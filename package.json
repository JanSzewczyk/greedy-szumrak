{
  "name": "greedy-szumrak",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "next build --turbopack",
    "dev": "next dev --turbopack",
    "lint": "next lint",
    "lint:ci": "next lint . -o eslint-results.sarif -f @microsoft/eslint-formatter-sarif",
    "lint:fix": "next lint --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "next start",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:serve": "serve storybook-static",
    "test": "vitest run --passWithNoTests",
    "test:ci": "CI=true vitest --passWithNoTests",
    "test:coverage": "vitest --coverage --passWithNoTests",
    "test:e2e": "playwright test --pass-with-no-tests",
    "test:e2e:ci": "CI=true playwright test --pass-with-no-tests",
    "test:e2e:ui": "playwright test --ui --pass-with-no-tests",
    "test:storybook": "vitest --project=storybook --coverage --passWithNoTests",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --project=unit --coverage --passWithNoTests",
    "test:watch": "vitest watch --passWithNoTests",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.33.3",
    "@clerk/themes": "^2.4.25",
    "@next/bundle-analyzer": "^15.5.4",
    "@szum-tech/design-system": "^2.4.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tailwindcss/postcss": "^4.1.14",
    "next": "^15.5.4",
    "next-compose-plugins": "^2.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.64.0",
    "server-only": "^0.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@playwright/test": "^1.56.0",
    "@storybook-community/storybook-dark-mode": "^6.0.8",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-vitest": "9.1.10",
    "@storybook/nextjs-vite": "^9.1.10",
    "@storybook/react": "^9.0.15",
    "@szum-tech/eslint-config": "^2.1.10",
    "@szum-tech/prettier-config": "^1.6.2",
    "@szum-tech/semantic-release-config": "^2.3.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^24.7.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.37.0",
    "playwright": "^1.56.0",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.9",
    "serve": "^14.2.5",
    "storybook": "^9.0.15",
    "storybook-addon-tag-badges": "^2.0.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
