{
  "name": "greedy-szumrak",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "next build --turbopack",
    "dev": "next dev --turbopack",
    "lint": "next lint",
    "lint:ci": "next lint . -o eslint-results.sarif -f @microsoft/eslint-formatter-sarif",
    "lint:fix": "next lint --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "next start",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:serve": "serve storybook-static",
    "test": "vitest run --passWithNoTests",
    "test:ci": "CI=true vitest --passWithNoTests",
    "test:coverage": "vitest --coverage --passWithNoTests",
    "test:e2e": "playwright test --pass-with-no-tests",
    "test:e2e:ci": "CI=true playwright test --pass-with-no-tests",
    "test:e2e:ui": "playwright test --ui --pass-with-no-tests",
    "test:storybook": "vitest --project=storybook --coverage --passWithNoTests",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --project=unit --coverage --passWithNoTests",
    "test:watch": "vitest watch --passWithNoTests",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.34.5",
    "@clerk/themes": "^2.4.33",
    "@hookform/resolvers": "^5.2.2",
    "@next/bundle-analyzer": "^16.0.1",
    "@szum-tech/design-system": "^3.2.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tailwindcss/postcss": "^4.1.17",
    "firebase-admin": "^13.6.0",
    "lucide-react": "^0.553.0",
    "next": "^16.0.1",
    "next-compose-plugins": "^2.2.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "server-only": "^0.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@playwright/test": "^1.56.1",
    "@storybook-community/storybook-dark-mode": "^6.0.8",
    "@storybook/addon-a11y": "^10.0.6",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/addon-vitest": "10.0.6",
    "@storybook/nextjs-vite": "^10.0.6",
    "@storybook/react": "^10.0.6",
    "@szum-tech/eslint-config": "^2.1.10",
    "@szum-tech/prettier-config": "^1.6.2",
    "@szum-tech/semantic-release-config": "^2.3.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/browser": "^4.0.8",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "eslint": "^9.39.1",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "semantic-release": "^25.0.2",
    "serve": "^14.2.5",
    "storybook": "^10.0.6",
    "storybook-addon-tag-badges": "^3.0.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8"
  }
}
